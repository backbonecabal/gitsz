{
  "name": "gitsz",
  "version": "2.3.2",
  "description": "Git tags with higher level of security",
  "main": "lib/gitsz.js",
  "bin": {
    "gitsz": "bin/gitsz"
  },
  "scripts": {
    "lint": "prettier --loglevel debug --write . ",
    "format": "npx eslint --debug --color --ext lib/gitsz.js",
    "report-test": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "coverage": "nyc npm run test",
    "test": "nyc tape tests/*.js | tap-nyc"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sambacha/gitsz.git"
  },
  "keywords": [
    "git",
    "secure",
    "tag",
    "sha256",
    "gpg",
    "signature",
    "verify",
    "sign"
  ],
  "author": "SEE CONTRIBUTORS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sambacha/gitsz/issues"
  },
  "homepage": "https://github.com/sambacha/gitsz#readme",
  "dependencies": {
    "async": "^3.2.0",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "codecov": "^3.8.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "tap-nyc": "^1.0.3",
    "tape": "^5.0.1"
  }
}
